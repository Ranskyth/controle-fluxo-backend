generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id Int @id @default(autoincrement())
  titulo String
  quantidade Int
  preco Float
  descricao String?
  createde_at DateTime @default(now())
  updated_at DateTime @updatedAt
  
  @@map("produtos")
}

model Admin{
  id String @id
  email String
  password String

  @@map("administrador")
}
